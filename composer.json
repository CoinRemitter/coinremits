{
    "name": "coinremitter/laravel",
    "description": "this is for direct use of apis of coinremitter into laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "laravel/framework": ">=5.2 <=8.0"
    },
    "autoload":{
        "psr-4":{
            "Coinremitter\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        },
        "laravel": {
            "providers": [
                "Coinremitter\\CoinremiterServiceProvider"
            ]
        }
    }
}
